@using CarInsurance.ViewModels
@model List<InsureeVm>

<h2>Insuree Quotes</h2>

<div>
    <h4>Insurees</h4>
    <hr />
    <table class="table">
        <tr>
            <th>
                FirstName
            </th>
            <th>
                LastName
            </th>
            <th>
                EmailAddress
            </th>
            <th>
                Quote
            </th>
        </tr>
        @foreach (var insuree in Model)
        {
            <tr>
                <td>
                    @insuree.FirstName
                </td>
                <td>
                    @insuree.LastName
                </td>
                <td>
                    @insuree.EmailAddress
                </td>
                <td>
                    $ @insuree.Quote
                </td>
            </tr>
        }
    </table>
</div>

